<!DOCTYPE html>
<html>
<head>
	<title>Edit Job</title>
</head>
<body>
	<form method="post">
		<fieldset>
			<legend>Edit Job</legend>
			<table>
				<tr>
					<td>Company Name</td>
					<td><input type="text" name="companyname" value="<%=companyname%>"></td>
				</tr>
				<tr>
					<td>Job Title</td>
					<td><input type="text" name="jobtitle" value="<%=jobtitle%>"></td>
				</tr>
				<tr>
					<td>Job Location</td>
					<td><input type="text" name="joblocation" value="<%=joblocation%>"></td>
				</tr>
				<tr>
					<td>Salary</td>
					<td><input type="password" name="salary" value="<%=salary%>"></td>
				</tr>
				<tr>
					<td><button type="submit">Confirm</button></td>
				</tr>
			</table>
		</fieldset>
	</form>
</body>
</html>