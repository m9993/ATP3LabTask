<!DOCTYPE html>
<html>
<head>
	<title>Admin</title>
</head>
<body>
	<h1>Admin Home</h1>
 	
	 <h3> Welcome, <%=profileName%></h3>
	 <br>
	<h5>USER:</h5>
	<a href="/user/create">Create User</a>
	| <a href="/logout">logout</a> 
	<br>

	<table border="1">
		<tr>
			<td>Id</td>
			<td>Name</td>
			<td>Company Name</td>
			<td>Contact no</td>
			<td>Username</td>
			<td>Role</td>
			<td colspan="2">Configaration</td>
		</tr>
		
		<!-- <% for(var i=0; i < users.length; i++){ %>
			
			<tr>
				<td><%= users[i][0] %></td>
				<td><%= users[i][1] %></td>
				<td><%= users[i][2] %></td>
				<td><%= users[i][3] %></td>
			</tr>

		<% } %> -->

		<% 
			users.forEach( function(std){ %>
			<tr>
				<td><%= std.id %></td>
				<td><%= std.name %></td>
				<td><%= std.companyname %></td>
				<td><%= std.contactno %></td>
				<td><%= std.username %></td>
				<td><%= std.role %></td>
				<td>
					<a href="/user/edit/<%= std.id %>">Edit</a> |
					<a href="/user/delete/<%= std.id %>">Delete</a>
				</td>
			</tr>

		<%
			}); 
		%>
		
	</table>

</body>
</html>